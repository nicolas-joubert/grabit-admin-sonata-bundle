{
  "name": "nicolas-joubert/grabit-admin-sonata-bundle",
  "description": "Sonata Admin for GrabitBundle",
  "keywords": ["grab", "crawler", "admin", "sonata"],
  "homepage": "https://github.com/nicolas-joubert/grabit-admin-sonata-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Joubert",
      "homepage": "https://www.nvirenque.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "NicolasJoubert\\GrabitAdminSonataBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NicolasJoubert\\GrabitAdminSonataBundle\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "ext-iconv": "*",
    "doctrine/dbal": "^3||^4",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/orm": "^3.0||^4.0",
    "nicolas-joubert/grabit-bundle": "^1.0",
    "sonata-project/admin-bundle": "^4.29",
    "sonata-project/doctrine-orm-admin-bundle": "^4.16",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "7.2.*",
    "symfony/runtime": "7.2.*",
    "symfony/twig-bundle": "7.2.*",
    "symfony/yaml": "7.2.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "*",
    "nicolas-joubert/grabit-front-feed-bundle": "dev-main",
    "phpstan/extension-installer": "*",
    "phpstan/phpstan": "*",
    "phpstan/phpstan-doctrine": "*",
    "phpstan/phpstan-symfony": "*",
    "phpunit/phpunit": "<10.0",
    "rector/rector": "*",
    "vincentlanglet/twig-cs-fixer": "*"
  },
  "suggest": {
    "nicolas-joubert/grabit-front-feed-bundle": "Atom feed frontend for GrabitBundle",
    "sonata-project/intl-bundle": "For better i18n support",
    "sonata-project/user-bundle": "For user and security management"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  }
}
